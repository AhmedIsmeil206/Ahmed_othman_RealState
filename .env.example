# API Configuration
REACT_APP_API_BASE_URL=http://localhost:8000/api/v1
REACT_APP_API_TIMEOUT=10000

# Authentication
REACT_APP_TOKEN_STORAGE_KEY=api_access_token

# Environment
REACT_APP_ENVIRONMENT=development

# Optional: API Features
REACT_APP_ENABLE_API_LOGGING=false
REACT_APP_ENABLE_RETRY_LOGIC=true
REACT_APP_MAX_RETRY_ATTEMPTS=3